{
  "name": "@platformatic/watt-admin",
  "version": "0.1.2",
  "scripts": {
    "prepublishOnly": "npm run build",
    "dev": "wattpm dev",
    "build": "wattpm build",
    "start": "wattpm start",
    "client": "./node_modules/.bin/plt-client ./web/backend/openapi.json --name backend --folder web/frontend/client --language ts --frontend --full --skip-config-update --props-optional",
    "test": "npm run test:cli && npm run test:backend && npm run test:frontend",
    "test:cli": "node --test test/cli.test.js test/start.test.js test/cli-integration.test.js",
    "test:backend": "cd web/backend && borp --concurrency 1",
    "test:frontend": "cd web/frontend && vitest run",
    "clean": "rm -rf ./web/*/dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.3.3",
    "@platformatic/composer": "^2.57.0",
    "@platformatic/runtime": "^2.57.0",
    "@platformatic/service": "^2.57.0",
    "@platformatic/control": "^2.57.0",
    "@platformatic/vite": "^2.57.0",
    "proxyquire": "^2.1.3",
    "split2": "4.2.0",
    "fastify": "^5.0.0",
    "wattpm": "^2.57.0"
  },
  "devDependencies": {
    "@fastify/type-provider-json-schema-to-ts": "^5.0.0",
    "@types/proxyquire": "1.3.31",
    "fastify-tsconfig": "^2.0.0",
    "borp": "^0.19.0",
    "@inquirer/testing": "^2.1.45",
    "@types/split2": "4.2.3",
    "eslint": "^9.22.0",
    "typescript": "^5.5.4",
    "neostandard": "^0.12.1",
    "@platformatic/client-cli": "^2.58.0",
    "@platformatic/ui-components": "^0.13.1",
    "@types/d3": "^7.4.3",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "d3": "~7.9.0",
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.0",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.4.15",
    "use-error-boundary": "^2.0.6",
    "vite": "^5.4.11",
    "vitest": "^3.0.8",
    "zustand": "^5.0.2"
  },
  "bin": {
    "watt-admin": "./cli.js"
  }
}
