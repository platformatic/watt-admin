{
  "$schema": "https://schemas.platformatic.dev/wattpm/2.71.0.json",
  "server": {
    "hostname": "127.0.0.1",
    "port": "{PORT}"
  },
  "logger": {
    "level": "info"
  },
  "autoload": {
    "path": "web",
    "mappings": {
      "backend": {
        "id": "backend",
        "useHttp": true
      }
    }
  }
}
